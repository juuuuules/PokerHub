{
    "editor.fontSize": 16,
    "editor.mouseWheelZoom": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.sortImports": true
    },
    "prettier.useTabs": true,
    "prettier.singleQuote": false,
    "prettier.endOfLine": "auto",
    "window.zoomLevel": 0

}